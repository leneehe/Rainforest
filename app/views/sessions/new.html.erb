<h2>Login</h2>
<%= form_for :session, url: sessions_path do |f| %>
<div class="field">
  <%= f.label :email %>
  <%= f.email_field :email %>
</div>
<div class="field">
  <%= f.label :password %>
  <%= f.password_field :password %>
</div>
<div class="action">
  <%= f.submit "Login" %>
</div>
<% end %>
